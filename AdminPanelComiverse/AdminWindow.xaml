<Window x:Class="AdminPanelComiverse.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:AdminPanelComiverse"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
                <Button Margin="5">Работа с таблицами</Button>
                <Button Margin="5">Бэкап БД</Button>
                <Button Margin="5">Журнал жалоб</Button>
                <Button Margin="5">Выйти</Button>
            </StackPanel>
            <Grid Margin="5"></Grid>
        </DockPanel>
    </Grid>
</Window>
